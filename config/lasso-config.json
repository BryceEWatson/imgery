    "bundlingEnabled": true,
    "minify": false,
    "cacheProfile": "development",
    "resolveCssUrls": true,
    "plugins": [
        "lasso-autoprefixer",
        "lasso-less",
        {
            "plugin": "lasso-require",
            "config": {
                "transforms": [
